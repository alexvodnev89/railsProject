<p id="notice"><%= notice %></p>

<h1>Listing Membership Options</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @membership_options.each do |membership_option| %>
      <tr>
        <td><%= membership_option.title %></td>
        <td><%= link_to 'Show', membership_option %></td>
        <td><%= link_to 'Edit', edit_membership_option_path(membership_option) %></td>
        <td><%= link_to 'Destroy', membership_option, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Membership option', new_membership_option_path %>
