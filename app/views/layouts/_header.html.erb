<header class="navbar navbar-fixed-top navbar-inverse">
  <div class="container">
    <% if logged_in? %>
        <% if admin_user? %>
          <h4 class="headerName">You are logged in as : <%= current_user.name %> |  ADMIN USER</h4>
        <% else %>
          <h4 class="headerName">You are logged in as : <%= current_user.name %></h4>
        <% end %>
    <% else %>
        <%= link_to "Users app", root_path, id: "logo" %>
    <% end %>
    <nav>
      <ul class="nav navbar-nav navbar-right">
        <li><%= link_to "Home", root_path %></li>
        <% if !admin_user? %>
          <li><%= link_to "Help", help_path %></li>
        <% end %> 
        <% if logged_in? %>
          <li><%= link_to "Membership", memberpage_path %></li>
          <% if admin_user? %>
            <li><%= link_to "Users", users_path %></li>
            <li><%= link_to "Contact", contact_path %></li>
          <% end %>
          <li class="dropdown">
            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
              Account <b class="caret"></b>
            </a>
            <ul class="dropdown-menu">
              <li><%= link_to "Profile", current_user %></li>
              <li><%= link_to "Settings", edit_user_path(current_user) %></li>
              <li class="divider"></li>
              <li>
                <%= link_to "Log out", logout_path, method: :delete %>
              </li>
            </ul>
          </li>
        <% else %>
          <li><%= link_to "Log in", login_path %></li>
        <% end %>
      </ul>
    </nav>
  </div>
</header>